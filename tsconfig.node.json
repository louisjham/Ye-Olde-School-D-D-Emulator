{
    "compilerOptions": {
        "target": "ES2022",
        "module": "ESNext",
        "lib": [
            "ES2022"
        ],
        "skipLibCheck": true,
        "types": [
            "node"
        ],
        "moduleResolution": "bundler",
        "allowSyntheticDefaultImports": true,
        "isolatedModules": true,
        "moduleDetection": "force",
        "noEmit": true
    },
    "include": [
        "vite.config.ts"
    ]
}